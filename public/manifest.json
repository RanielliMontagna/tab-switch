{
  "manifest_version": 3,
  "name": "Tab Switch",
  "version": "1.0.0",
  "description": "Switch between tabs automatically",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "favicon.ico",
    "48": "favicon.ico",
    "128": "favicon.ico"
  }
}
