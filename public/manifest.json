{
  "manifest_version": 3,
  "name": "Tab Switch",
  "version": "1.0.0",
  "description": "Switch between tabs automatically",
  "permissions": ["storage", "tabs"],
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
